<fragment  xmlns='http://www.xml-cml.org/schema'
  xmlns:g='http://www.xml-cml.org/mols/geom1'>
<!--// polyinfo examples-->
  <fragmentList>
    <fragment id='p129'>
      <molecule ref='g:ch2'/>
    </fragment>
    <fragment id='p708'>
      <molecule ref='g:benzene'/>
    </fragment>
  </fragmentList>
  <fragment countExpression='*(10)'>
    <join atomRefs2='r1 r2' moleculeRefs2='PREVIOUS NEXT'>
      <torsion>180</torsion>
    </join>
    <fragment>
      <fragment>
	<molecule ref='g:ch'>
	  <join moleculeRefs2='PARENT CHILD' atomRefs2='r3 r1'>
	    <fragment ref='p708'/>
	  </join>
	</molecule>
      </fragment>
      <join atomRefs2='r1 r2' moleculeRefs2='PREVIOUS NEXT'/>
      <fragment ref='p129'/>
    </fragment>
  </fragment>
</fragment>

