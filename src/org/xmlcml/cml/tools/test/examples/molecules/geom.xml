 <!-- acetate -->
 <molecule role="fragment" id="acet">
  <arg parameterName="idx"/>
  <arg parentAttribute="id">acet_{$idx}</arg>
  <atomArray>
   <atom id="a1" elementType="C" hydrogenCount="3">
    <arg parentAttribute="id">acet_{$idx}_a1</arg>
   </atom>
   <atom id="a2" elementType="C" hydrogenCount="0">
    <arg parentAttribute="id">acet_{$idx}_a2</arg>
   </atom>
   <atom id="a3" elementType="O" hydrogenCount="0">
    <arg parentAttribute="id">acet_{$idx}_a3</arg>
   </atom>
   <atom id="a4" elementType="O" hydrogenCount="0">
    <arg parentAttribute="id">acet_{$idx}_a4</arg>
   </atom>
   <atom id="r1" elementType="R">
    <arg parentAttribute="id">acet_{$idx}_r1</arg>
   </atom>
  </atomArray>
  <bondArray>
    <bond atomRefs2="a1 a2" order="1"
      ><arg parentAttribute="id">acet_{$idx}_a1_acet_{$idx}_a2</arg
      ><arg parentAttribute="atomRefs2">acet_{$idx}_a1 acet_{$idx}_a2</arg>
    </bond>
    <bond atomRefs2="a2 a3" order="2"
      ><arg parentAttribute="id">acet_{$idx}_a2_acet_{$idx}_a3</arg
      ><arg parentAttribute="atomRefs2">acet_{$idx}_a2 acet_{$idx}_a3</arg>
    </bond>
    <bond atomRefs2="a2 a4" order="1"
      ><arg parentAttribute="id">acet_{$idx}_a2_acet_{$idx}_a4</arg
      ><arg parentAttribute="atomRefs2">acet_{$idx}_a2 acet_{$idx}_a4</arg>
    </bond>
    <bond atomRefs2="a4 r1" order="1"
      ><arg parentAttribute="id">acet_{$idx}_a4_acet_{$idx}_r1</arg
      ><arg parentAttribute="atomRefs2">acet_{$idx}_a4 acet_{$idx}_r1</arg>
    </bond>
  </bondArray>
  <name><arg substitute=".">acet_{$idx}</arg></name>
<length atomRefs2="a1 a2"
  ><arg parentAttribute="atomRefs2">acet_{$idx}_a1 acet_{$idx}_a2</arg>1.52</length>
<length atomRefs2="a2 a3"
  ><arg parentAttribute="atomRefs2">acet_{$idx}_a2 acet_{$idx}_a3</arg>1.25</length>
<angle atomRefs3="a4 a2 a3"
  ><arg parentAttribute="atomRefs3">acet_{$idx}_a4 acet_{$idx}_a2 acet_{$idx}_a3</arg>119</angle>
<length atomRefs2="a2 a4"
  ><arg parentAttribute="atomRefs2">acet_{$idx}_a2 acet_{$idx}_a4</arg>1.40</length>
<angle atomRefs3="a1 a2 a4"
  ><arg parentAttribute="atomRefs3">acet_{$idx}_a1 acet_{$idx}_a2 acet_{$idx}_a4</arg>121</angle>
<torsion atomRefs4="a1 a4 a2 a3"
  ><arg parentAttribute="atomRefs4">acet_{$idx}_a1 acet_{$idx}_a4 acet_{$idx}_a2 acet_{$idx}_a3</arg>180</torsion>
<angle atomRefs3="a2 a4 r1"
  ><arg parentAttribute="atomRefs3">acet_{$idx}_a2 acet_{$idx}_a4 acet_{$idx}_r1</arg>120</angle>
<torsion atomRefs4="a1 a2 a4 r1"
  ><arg parentAttribute="atomRefs4">acet_{$idx}_a1 acet_{$idx}_a2 acet_{$idx}_a4 acet_{$idx}_r1</arg>180</torsion>
</molecule>

